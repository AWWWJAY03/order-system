# Environment variables for J&T Express automation
# Copy this file to .env and fill in your actual credentials

# J&T Express Portal Credentials
JT_EMAIL=your-jt-email@example.com
JT_PASSWORD=your-jt-password

# Business Information (Sender Details)
SENDER_NAME=Your Business Name
SENDER_CONTACT=09123456789
SENDER_ADDRESS=Your Business Address, City, Province, ZIP Code
SENDER_COMPANY=Your Company Name

# Google Apps Script Webhook URL (from your deployed web app)
APPS_SCRIPT_WEBHOOK=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Server Configuration
PORT=3000

# Optional: Enable debug mode
DEBUG=true