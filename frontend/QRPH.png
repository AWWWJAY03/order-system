<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="200" height="200" fill="#1e3a8a" rx="8"/>
  
  <!-- GCash branding -->
  <rect x="10" y="10" width="180" height="40" fill="#ffffff" rx="4"/>
  <text x="100" y="35" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1e3a8a">GCash QR</text>
  
  <!-- QR Code pattern simulation -->
  <g transform="translate(25, 60)">
    <!-- Corner markers -->
    <rect x="0" y="0" width="21" height="21" fill="white"/>
    <rect x="3" y="3" width="15" height="15" fill="#1e3a8a"/>
    <rect x="6" y="6" width="9" height="9" fill="white"/>
    
    <rect x="129" y="0" width="21" height="21" fill="white"/>
    <rect x="132" y="3" width="15" height="15" fill="#1e3a8a"/>
    <rect x="135" y="6" width="9" height="9" fill="white"/>
    
    <rect x="0" y="129" width="21" height="21" fill="white"/>
    <rect x="3" y="132" width="15" height="15" fill="#1e3a8a"/>
    <rect x="6" y="135" width="9" height="9" fill="white"/>
    
    <!-- Data pattern -->
    <rect x="30" y="6" width="3" height="3" fill="white"/>
    <rect x="36" y="6" width="3" height="3" fill="white"/>
    <rect x="42" y="6" width="3" height="3" fill="white"/>
    <rect x="54" y="6" width="3" height="3" fill="white"/>
    <rect x="66" y="6" width="3" height="3" fill="white"/>
    
    <rect x="30" y="12" width="3" height="3" fill="white"/>
    <rect x="42" y="12" width="3" height="3" fill="white"/>
    <rect x="48" y="12" width="3" height="3" fill="white"/>
    <rect x="60" y="12" width="3" height="3" fill="white"/>
    
    <!-- More pattern elements -->
    <rect x="6" y="30" width="3" height="3" fill="white"/>
    <rect x="12" y="30" width="3" height="3" fill="white"/>
    <rect x="30" y="30" width="3" height="3" fill="white"/>
    <rect x="36" y="30" width="3" height="3" fill="white"/>
    <rect x="48" y="30" width="3" height="3" fill="white"/>
    
    <!-- Central pattern -->
    <rect x="66" y="66" width="18" height="18" fill="white"/>
    <rect x="69" y="69" width="12" height="12" fill="#1e3a8a"/>
    <rect x="72" y="72" width="6" height="6" fill="white"/>
    
    <!-- Additional data patterns -->
    <rect x="6" y="48" width="3" height="3" fill="white"/>
    <rect x="18" y="48" width="3" height="3" fill="white"/>
    <rect x="30" y="48" width="3" height="3" fill="white"/>
    <rect x="42" y="48" width="3" height="3" fill="white"/>
    
    <rect x="6" y="60" width="3" height="3" fill="white"/>
    <rect x="12" y="60" width="3" height="3" fill="white"/>
    <rect x="24" y="60" width="3" height="3" fill="white"/>
    <rect x="36" y="60" width="3" height="3" fill="white"/>
    
    <!-- Bottom pattern -->
    <rect x="30" y="132" width="3" height="3" fill="white"/>
    <rect x="36" y="132" width="3" height="3" fill="white"/>
    <rect x="48" y="132" width="3" height="3" fill="white"/>
    <rect x="54" y="132" width="3" height="3" fill="white"/>
    
    <rect x="30" y="138" width="3" height="3" fill="white"/>
    <rect x="42" y="138" width="3" height="3" fill="white"/>
    <rect x="54" y="138" width="3" height="3" fill="white"/>
    <rect x="60" y="138" width="3" height="3" fill="white"/>
  </g>
  
  <!-- Footer text -->
  <text x="100" y="185" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="white">Scan with GCash App</text>
</svg>